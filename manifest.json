{
  "name": "NetflixSkipper",
  "version": "1.0",
  "description": "Awsome One",
  "browser_action": {
    "default_title": "choose filtering level",
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.netflix.com/*"
      ],
      "js": [
        "netflix.js"
      ],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2
}